{
    "jsxSingleQuote": true,
    "singleQuote": true,
    "semi": true,
    "tabWidth": 4,
    "trailingComma": "all",
    "printWidth": 100,
    "bracketSameLine": false,
    "useTabs": false,
    "arrowParens": "always",
    "endOfLine": "auto",
    "importOrder": [
        "^react.*$",
        "<THIRD_PARTY_MODULES>",
        "^@insync/(.*)$",
        "^../.*$",
        "^./.*$"
    ],
    "plugins": [
        "@trivago/prettier-plugin-sort-imports"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
}